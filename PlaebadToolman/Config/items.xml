	<set xpath="/items/item[@name='meleeToolShovelT0StoneShovel']/property[@name='Tags']/@value">melee,grunting,medium,tool,longShaft,shovel,attStrength,perkMiner69r,perkMotherLode,canHaveCosmetic,harvestingSkill,perkToolman</set>
	<set xpath="/items/item[@name='meleeToolShovelT1IronShovel']/property[@name='Tags']/@value">melee,grunting,medium,tool,longShaft,shovel,attStrength,perkMiner69r,perkMotherLode,canHaveCosmetic,harvestingSkill,perkToolman</set>
	<set xpath="/items/item[@name='meleeToolShovelT2SteelShovel']/property[@name='Tags']/@value">melee,grunting,medium,tool,longShaft,shovel,attStrength,perkMiner69r,perkMotherLode,canHaveCosmetic,harvestingSkill,perkToolman</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/property[@name='Tags']/@value">weapon,ranged,tool,nailgun,sideAttachments,repairTool,attStrength,perkMiner69r,canHaveCosmetic,repairingSkill,magazine,drumMagazine,stock,firingMode,perkToolman,attachmentsIncluded</set>
	<set xpath="/items/item[@name='meleeToolAxeT3Chainsaw']/property[@name='Tags']/@value">melee,heavy,tool,motorTool,attStrength,perkMiner69r,perkMotherLode,chainsaw,canHaveCosmetic,harvestingSkill,perkToolman</set>
	<set xpath="/items/item[@name='meleeToolPickT3Auger']/property[@name='Tags']/@value">melee,heavy,tool,motorTool,attStrength,perkMiner69r,perkMotherLode,miningTool,canHaveCosmetic,harvestingSkill,perkToolman</set>

	<!-- Update tags on harvest tools so that Big and Fast perk applies -->
	<set xpath="/items/item[@name='meleeToolRepairT0StoneAxe']/effect_group[@name='meleeToolRepairT0StoneAxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,axe,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolRepairT0TazasStoneAxe']/effect_group[@name='meleeToolRepairT0TazasStoneAxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,axe,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolRepairT1ClawHammer']/effect_group[@name='meleeToolRepairT1ClawHammer']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolAxeT1IronFireaxe']/effect_group[@name='meleeToolAxeT1IronFireaxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,axe,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolAxeT2SteelAxe']/effect_group[@name='meleeToolAxeT2SteelAxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,axe,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolPickT1IronPickaxe']/effect_group[@name='meleeToolPickT1IronPickaxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolPickT2SteelPickaxe']/effect_group[@name='meleeToolPickT2SteelPickaxe']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolShovelT0StoneShovel']/effect_group[@name='meleeToolShovelT0StoneShovel']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolShovelT1IronShovel']/effect_group[@name='meleeToolShovelT1IronShovel']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>
	<set xpath="/items/item[@name='meleeToolShovelT2SteelShovel']/effect_group[@name='meleeToolShovelT2SteelShovel']/passive_effect[@name='AttacksPerMinute']/@tags">perkMiner69r,perkFlurryOfStrength</set>

	<!-- Add Target Armor Reduction to the Iron and Steel Pickaxe -->
	<insertAfter xpath="/items/item[@name='meleeToolPickT1IronPickaxe']/effect_group[@name='meleeToolPickT1IronPickaxe']/passive_effect[@name='AttacksPerMinute']">
		<passive_effect name="TargetArmor" operation="perc_add" value="-.4" tags="perkMiner69er"/>
		<display_value name="dTargetArmor" value="-.4"/>
	</insertAfter>
	<insertAfter xpath="/items/item[@name='meleeToolPickT2SteelPickaxe']/effect_group[@name='meleeToolPickT2SteelPickaxe']/passive_effect[@name='AttacksPerMinute']">
		<passive_effect name="TargetArmor" operation="perc_add" value="-.5" tags="perkMiner69er"/>
		<display_value name="dTargetArmor" value="-.5"/>
	</insertAfter>

	<!-- Change Pickaxe display type to show armor reduction -->
	<set xpath="/items/item[@name='meleeToolPickT1IronPickaxe']/property[@name='DisplayType']/@value">meleeSpear</set>
	<set xpath="/items/item[@name='meleeToolPickT2SteelPickaxe']/property[@name='DisplayType']/@value">meleeSpear</set>

	<!-- Increase entity damage of the pickaxes and shovel -->
	<set xpath="/items/item[@name='meleeToolPickT1IronPickaxe']/effect_group[@name='meleeToolPickT1IronPickaxe']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">15</set>
	<set xpath="/items/item[@name='meleeToolPickT2SteelPickaxe']/effect_group[@name='meleeToolPickT2SteelPickaxe']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">28</set>
	<set xpath="/items/item[@name='meleeToolShovelT1IronShovel']/effect_group[@name='meleeToolShovelT1IronShovel']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">18</set>
	<set xpath="/items/item[@name='meleeToolShovelT2SteelShovel']/effect_group[@name='meleeToolShovelT2SteelShovel']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">28</set>

	<!-- Change nailgun action class to Ranged to correct burst firing mode behavior -->
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/property[@class='Action0']/property[@name='Class']/@value">Ranged</set>

	<!-- Add damage scaling and bonuses for Nailgun -->
	<insertAfter xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='ModSlots']">
		<passive_effect name="ModPowerBonus" operation="perc_add" value=".10" tags="EntityDamage,BlockDamage"/>
		<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
		<passive_effect name="EntityDamage" operation="base_add" value="5" tags="perkMiner69r"/>
		<passive_effect name="EntityDamage" operation="perc_add" value="-.02,.02" tags="perkMiner69r"/>
		<passive_effect name="EntityDamage" operation="perc_add" value=".05,.25" tier="2,6" tags="perkMiner69r"/>
		<passive_effect name="BlockDamage" operation="perc_add" value="-.08,.08" tags="perkMiner69r"/>
		<passive_effect name="BlockDamage" operation="perc_add" value=".15,.75" tier="2,6" tags="perkMiner69r"/>
	</insertAfter>

	<!-- Increase magazine size of the nailgun -->
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='BurstRoundCount']/@value">3</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='MagazineSize']/@value">50</set>

	<!-- Adjust nailgun damage and handling -->
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='DamageFalloffRange' and @operation='base_set']/@value">20</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='MaxRange']/@value">40</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='KickDegreesVerticalMin']/@value">-.25</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='KickDegreesVerticalMax']/@value">.25</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='KickDegreesHorizontalMin']/@value">-.3</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='KickDegreesHorizontalMax']/@value">.3</set>
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='IncrementalSpreadMultiplier']/@value">1.5</set>

	<!-- Enable mod slots on the nailgun -->
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='ModSlots']/@value">3,3,3,4,4,4</set>
	<setattribute xpath="/items/item[@name='meleeToolRepairT3Nailgun']/effect_group[@name='meleeToolRepairT3Nailgun']/passive_effect[@name='ModSlots']" name="tier">1,2,3,4,5,6</setattribute>

	<!-- Update nailgun to work with weapon flashlight / laser sight mod -->
	<insertAfter xpath="/items/item[@name='meleeToolRepairT3Nailgun']/property[@name='SoundPlace']">
		<property name="Attachments" value="meleeToolFlashlight02"/>
		<property name="LightSource" value="lightSource"/> <!-- Start: Needed for the attachment flashlight -->
		<property name="ActivateObject" value="Attachments/flashlight/lightSource"/>
		<property name="AttachmentFlashlight" value="flashlight02"/> <!-- End: Needed for the attachment flashlight -->
		<property name="LightValue" value=".45"/>
	</insertAfter>

	<!-- Increase base entity damage and velocity of nails -->
	<set xpath="/items/item[@name='resourceNail']/effect_group[@name='resourceNail']/passive_effect[@name='EntityDamage']/@value">25</set>
	<set xpath="/items/item[@name='resourceNail']/effect_group[@name='resourceNail']/passive_effect[@name='ProjectileVelocity']/@value">45</set>

	<!-- Add nails to ammo groups -->
	<set xpath="/items/item[@name='resourceNail']/property[@name='Group']/@value">Resources,Ammo/Weapons,Basics,Ammo,Ranged Weapons</set>

	<!-- Add steel nails to nailgun ammo -->
	<set xpath="/items/item[@name='meleeToolRepairT3Nailgun']/property[@class='Action0']/property[@name='Magazine_items']/@value">resourceNail, resourceNailSteel</set>

	<!-- Reduce stamina usage of the Impact Driver -->
	<set xpath="/items/item[@name='meleeToolSalvageT3ImpactDriver']/effect_group[@name='meleeToolSalvageT3ImpactDriver']/passive_effect[@name='StaminaLoss']/@value">16</set>
	<set xpath="/items/item[@name='meleeToolSalvageT3ImpactDriver']/effect_group[@name='Power Attack']/passive_effect[@name='StaminaLoss']/@value">28</set>

	<!-- Update damage and handling on the hammer -->
	<set xpath="/items/item[@name='meleeToolRepairT1ClawHammer']/effect_group[@name='meleeToolRepairT1ClawHammer']/passive_effect[@name='AttacksPerMinute']/@value">90</set>

	<!-- Update hammer tags to allow installing the stun repulsor mod -->
	<set xpath="/items/item[@name='meleeToolRepairT1ClawHammer']/property[@name='Tags']/@value">melee,light,tool,repairTool,longshaft,attStrength,perkMiner69r,perkMotherLode,canHaveCosmetic,repairingTools,clawHammer</set>

	<!-- Add charges for stun repulsor mod to claw hammer -->
	<insertAfter xpath="/items/item[@name='meleeToolRepairT1ClawHammer']/effect_group[@name='meleeToolRepairT1ClawHammer']">
		<!-- Show spark effect if fully charged -->
		<effect_group name="applySparkEffect">
			<triggered_effect trigger="onSelfEquipStart" action="AddPart" part="Sparks" prefab="@:ParticleEffects/p_electric_shock_small.prefab" parentTransform="Handle" localPos="0,0,0" localRot="0,0,0">
				<requirement name="CompareItemMetaFloat" operation="GTE" value="4" key="charge"/></triggered_effect>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
			<triggered_effect trigger="onSelfEquipStop" action="RemovePart" part="Sparks"/>
		</effect_group>
		<effect_group name="increase charges">
			<!-- initialize metadata for "charge" on claw hammer, don't change value if already exists -->
			<triggered_effect trigger="onSelfEquipStart" action="SetItemMetaFloat" change="0" relative="true" key="charge">
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
			</triggered_effect>
			
			<!-- play stun baton sounds -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" mode="before" action="PlaySound" sound="stunbaton_hit1">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="CompareItemMetaFloat" operation="Equals" value="0" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			<triggered_effect trigger="onSelfPrimaryActionRayHit" mode="before" action="PlaySound" sound="stunbaton_hit2">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="CompareItemMetaFloat" operation="Equals" value="1" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			<triggered_effect trigger="onSelfPrimaryActionRayHit" mode="before" action="PlaySound" sound="stunbaton_hit3">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="CompareItemMetaFloat" operation="Equals" value="2" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			<triggered_effect trigger="onSelfPrimaryActionRayHit" mode="before" action="PlaySound" sound="stunbaton_hit4">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="CompareItemMetaFloat" operation="Equals" value="3" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			<triggered_effect trigger="onSelfPrimaryActionRayHit" mode="before" action="PlaySound" sound="stunbaton_hit5">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="CompareItemMetaFloat" operation="Equals" value="4" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>

			<!-- potentially apply bonus charges from TechJunkie perk, by setting it to 3 the other actions below increase it to max charge -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="SetItemMetaFloat" change="3" relative="false" key="charge">
				<requirement name="CompareItemMetaFloat" operation="LTE" value="3" key="charge"/>
				<requirement name="ProgressionLevel" progression_name="perkTechJunkie6BatonCharge" operation="Equals" value="1"/>
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="RandomRoll" seed_type="Random" min_max="0,100" operation="LTE" value="25"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
			</triggered_effect>
			
			<!-- increase charges -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="SetItemMetaFloat" change="1" relative="true" key="charge">
				<requirement name="IsAlive" target="other"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
			</triggered_effect>

			<!-- add electric effect to claw hammer -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="AddPart" part="Sparks" prefab="@:ParticleEffects/p_electric_shock_small.prefab" parentTransform="Handle" localPos="0,0,0" localRot="0,0,0">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="4" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			
			
			<!-- trigger ragdoll events for fully charged claw hammer -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="Ragdoll" target="other" duration="@stunBatonDuration" force="350">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="5" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
				<requirement name="!EntityTagCompare" target="other" tags="trader"/>
			</triggered_effect>
			
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="Ragdoll" target="otherAOE" range="1.1" duration="2.5" force="350" target_tags="zombie,animal">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="5" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
				<requirement name="!HasBuff" buff="buffDrugNerdTats"/>
			</triggered_effect>
			
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="Ragdoll" target="otherAOE" range="1.1" duration="@stunBatonDuration" force="350" target_tags="zombie,animal">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="5" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/>
				<requirement name="HasBuff" buff="buffDrugNerdTats"/> <!-- if you have buffDrugNerdTats, use stun duration so zombies get up after stun is finished -->
			</triggered_effect>
			
			<!-- reset charges -->
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="RemovePart" part="Sparks">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="5" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
			<triggered_effect trigger="onSelfPrimaryActionRayHit" action="SetItemMetaFloat" change="0" relative="false" key="charge">
				<requirement name="CompareItemMetaFloat" operation="Equals" value="5" key="charge"/>
				<requirement name="HasBuff" buff="buffmodMeleeStunBatonRepulsor"/></triggered_effect>
		</effect_group>
	</insertAfter>

	<!-- Add new item Steel Nail ammo -->
	<insertAfter xpath="/items/item[@name='resourceNail']">
		<item name="resourceNailSteel">
			<property name="Meshfile" value="@:Other/Items/Tools/nailProjectilePrefab.prefab"/>
			<property name="HoldType" value="45"/>
			<property name="HandMeshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
			<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
			<property name="Material" value="MresourceScrapIron"/>
			<property name="Stacknumber" value="1000"/> <!-- STK resource -->
			<property name="EconomicValue" value="10"/>
			<property name="EconomicBundleSize" value="10"/>
			<property name="Tags" value="ammo,ranged,attStrength,perkMiner69r,perkPenetrator"/>
			<property name="Group" value="Resources,Ammo/Weapons,Basics,Ammo,Ranged Weapons"/>
			<property name="SoundPickup" value="nails_grab"/>
			<property name="SoundPlace" value="nails_place"/>
			<property name="CustomIcon" value="resourceNail"/>
			<property name="CustomIconTint" value="ffb0b0"/>
			<property class="Action1">
				<property name="Class" value="Projectile"/>
				<property name="Explosion.ParticleIndex" value="0"/>
				<!-- no expl. -->
				<property name="FlyTime" value="1"/>
				<property name="LifeTime" value=".75"/>
			</property>
			<effect_group name="resourceNailSteel" tiered="false"> <!-- nail -->
				<passive_effect name="EntityDamage" operation="base_set" value="42"/>
				<passive_effect name="BlockDamage" operation="base_set" value="5"/>
				<passive_effect name="TargetArmor" operation="perc_add" value="-.5"/>
				<display_value name="dTargetArmor" value="-.5"/>
				<passive_effect name="EntityPenetrationCount" operation="base_set" value="1"/>
				<display_value name="dTargetPenetration" value="1"/>
				<passive_effect name="DamageModifier" operation="perc_set" value="0" tags="earth,metal,cloth"/>
				<passive_effect name="DamageModifier" operation="perc_set" value=".1" tags="wood"/>
				<passive_effect name="ProjectileVelocity" operation="base_set" value="55"/>
				<passive_effect name="ProjectileStickChance" operation="perc_set" value="0"/>
			</effect_group>
			<effect_group name="BlockPenetration" tiered="false">
				<passive_effect name="EntityPenetrationCount" operation="base_set" value="1" tags="perkPenetrator">
					<requirement name="ItemHasTags" tags="perkPenetrator"/>
					<!--<requirement name="ProgressionLevel" progression_name="perkPenetrator" operation="Equals" value="4"/>-->
				</passive_effect>

				<passive_effect name="BlockPenetrationFactor" operation="base_set" value="10000" tags="perkPenetrator"/>
			</effect_group>
			<effect_group name="perkPenetrator" tiered="false">
				<passive_effect name="TargetArmor" operation="perc_add" value="-.1" tags="perkPenetrator">
					<requirement name="ProgressionLevel" progression_name="perkPenetrator" operation="Equals" value="1"/></passive_effect>
				<passive_effect name="TargetArmor" operation="perc_add" value="-.15" tags="perkPenetrator">
					<requirement name="ProgressionLevel" progression_name="perkPenetrator" operation="Equals" value="2"/></passive_effect>
				<passive_effect name="TargetArmor" operation="perc_add" value="-.2" tags="perkPenetrator">
					<requirement name="ProgressionLevel" progression_name="perkPenetrator" operation="Equals" value="3"/></passive_effect>
				<passive_effect name="TargetArmor" operation="perc_add" value="-.3" tags="perkPenetrator">
					<requirement name="ProgressionLevel" progression_name="perkPenetrator" operation="Equals" value="4"/></passive_effect>
			</effect_group>
		</item>
	</insertAfter>

</PlaebadToolmanItems>